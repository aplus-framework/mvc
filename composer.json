{
    "name": "aplus/mvc",
    "description": "Aplus Framework MVC Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "app",
        "application",
        "mvc",
        "model",
        "view",
        "controller",
        "entity",
        "layout",
        "pages",
        "api",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/mvc",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/mvc/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://gitlab.com/aplus-framework/libraries/mvc",
        "docs": "https://docs.aplus-framework.com/guides/libraries/mvc/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "aplus/autoload": "^1.7",
        "aplus/cache": "^2.2",
        "aplus/cli": "^1.17",
        "aplus/config": "^2.1",
        "aplus/database": "^2.10",
        "aplus/date": "^1.4",
        "aplus/debug": "^2.3",
        "aplus/email": "^2.2",
        "aplus/http": "^4.5",
        "aplus/language": "^1.7",
        "aplus/log": "^2.2",
        "aplus/pagination": "^2.4",
        "aplus/routing": "^2.5",
        "aplus/session": "^2.5",
        "aplus/validation": "^1.10"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.12",
        "ergebnis/composer-normalize": "^2.23",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.11",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "aplus/coding-standard": "Aplus Framework Coding Standard Library",
        "aplus/front": "Aplus Framework Front Library",
        "aplus/testing": "Aplus Framework Testing Library"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\MVC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MVC\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
