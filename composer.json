{
    "name": "aplus/mvc",
    "type": "library",
    "description": "Aplus Framework MVC Library",
    "keywords": [
        "app",
        "application",
        "mvc",
        "model",
        "view",
        "controller",
        "entity",
        "layout",
        "pages",
        "api",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "homepage": "https://aplus-framework.com/docs/mvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "aplus/autoload": "^1.6",
        "aplus/cache": "^2.2",
        "aplus/cli": "^1.14",
        "aplus/config": "^1.4",
        "aplus/database": "^2.8",
        "aplus/date": "^1.4",
        "aplus/debug": "^2.2",
        "aplus/email": "^2.2",
        "aplus/http": "^4.0",
        "aplus/http-client": "dev-master",
        "aplus/image": "^2.1",
        "aplus/language": "^1.6",
        "aplus/log": "^2.2",
        "aplus/pagination": "^2.3",
        "aplus/routing": "^2.1",
        "aplus/session": "dev-master",
        "aplus/validation": "^1.9"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\MVC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MVC\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/mvc/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/mvc/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/mvc",
        "docs": "https://aplus-framework.gitlab.io/libraries/mvc/docs/"
    }
}
