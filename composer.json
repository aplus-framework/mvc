{
	"name": "framework/mvc",
	"type": "library",
	"description": "The Framework MVC Library",
	"keywords": [
		"app",
		"application",
		"mvc",
		"model",
		"view",
		"controller",
		"entity",
		"layout",
		"pages",
		"api",
		"resource"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/mvc.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"framework/autoload": "^1.0",
		"framework/cache": "^1.0",
		"framework/cli": "^1.0",
		"framework/database": "^1.0",
		"framework/date": "^1.0",
		"framework/email": "dev-master",
		"framework/http": "^1.0",
		"framework/language": "^1.0",
		"framework/pagination": "^1.0",
		"framework/routing": "^1.0",
		"framework/session": "^1.0",
		"framework/validation": "^1.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"framework/coding-standard": "^1.0",
		"localheinz/composer-normalize": "^1.3",
		"phpmd/phpmd": "^2.7",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^8.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\MVC\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\MVC\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/mvc/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/mvc/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/mvc",
		"docs": "https://the-framework.gitlab.io/libraries/mvc/docs/"
	}
}
