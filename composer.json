{
    "name": "natanfelles/mvc",
    "type": "library",
    "description": "The Framework MVC Library",
    "keywords": [
        "app",
        "application",
        "mvc",
        "model",
        "view",
        "controller",
        "entity",
        "layout",
        "pages",
        "api",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "homepage": "https://the-framework.gitlab.io/libraries/mvc.html",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "natanfelles/autoload": "^1.5",
        "natanfelles/cache": "^2.1",
        "natanfelles/cli": "^1.13",
        "natanfelles/config": "^1.3",
        "natanfelles/database": "^2.7",
        "natanfelles/database-extra": "^2.1",
        "natanfelles/date": "^1.3",
        "natanfelles/debug": "^2.1",
        "natanfelles/email": "^2.1",
        "natanfelles/http": "^3.10",
        "natanfelles/http-client": "^2.3",
        "natanfelles/image": "^2",
        "natanfelles/language": "^1.4",
        "natanfelles/log": "^2.1",
        "natanfelles/pagination": "^2.2",
        "natanfelles/routing": "^2",
        "natanfelles/session": "^2.3",
        "natanfelles/validation": "^1.6"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-xdebug": "*",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "natanfelles/coding-standard": "^1.7",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\MVC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MVC\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/the-framework/libraries/mvc/issues",
        "wiki": "https://gitlab.com/the-framework/libraries/mvc/wikis/home",
        "source": "https://gitlab.com/the-framework/libraries/mvc",
        "docs": "https://the-framework.gitlab.io/libraries/mvc/docs/"
    }
}
