{
    "name": "aplus/mvc",
    "description": "Aplus Framework MVC Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "app",
        "application",
        "mvc",
        "model",
        "view",
        "controller",
        "entity",
        "layout",
        "pages",
        "api",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/mvc",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/mvc/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://gitlab.com/aplus-framework/libraries/mvc",
        "docs": "https://docs.aplus-framework.com/guides/libraries/mvc/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-mysqli": "*",
        "aplus/autoload": "^2.0",
        "aplus/cache": "^3.0",
        "aplus/cli": "^2.3",
        "aplus/config": "^3.0",
        "aplus/database": "^3.2",
        "aplus/database-extra": "^3.0",
        "aplus/date": "^2.0",
        "aplus/debug": "^3.3",
        "aplus/email": "^3.1",
        "aplus/http": "^5.2",
        "aplus/language": "^3.0",
        "aplus/log": "^3.0",
        "aplus/pagination": "^3.2",
        "aplus/routing": "^3.4",
        "aplus/session": "^3.2",
        "aplus/validation": "^2.1"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.14",
        "ergebnis/composer-normalize": "^2.28",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\MVC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MVC\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
